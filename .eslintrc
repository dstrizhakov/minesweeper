{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/no-extraneous-dependencies": ["off"],
    "import/extensions": "off",
    "no-plusplus": [2, { "allowForLoopAfterthoughts": true }],
    "comma-dangle": "off"
  }
}
